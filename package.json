{
  "name": "ayo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node build/bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "docs:generate": "node ace docs:generate",
    "test:aidb-prod": "node --loader ts-node-maintained/esm script/test_aidb_prod_connection.ts",
    "worker": "node ace queue:worker",
    "worker:email": "node ace queue:worker --workers=email",
    "worker:image": "node ace queue:worker --workers=image",
    "queue:delayed": "node ace queue:process-delayed"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#workers/*": "./app/workers/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js",
    "#routes/*": "./app/routes/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^2.0.0",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/api-client": "^3.0.3",
    "@japa/assert": "^4.0.1",
    "@japa/plugin-adonisjs": "^4.0.0",
    "@japa/runner": "^4.2.0",
    "@swc/core": "1.10.7",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.13.2",
    "@types/ws": "^8.18.1",
    "eslint": "^9.20.1",
    "hot-hook": "^0.4.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.0",
    "ssh2": "^1.15.0",
    "ts-node-maintained": "^10.9.5",
    "typescript": "~5.7"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.3.1",
    "@adonisjs/core": "^6.17.2",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/i18n": "^2.2.0",
    "@adonisjs/limiter": "^2.4.0",
    "@adonisjs/lock": "^1.1.1",
    "@adonisjs/lucid": "^21.6.0",
    "@adonisjs/redis": "^9.2.0",
    "@google/genai": "^1.30.0",
    "@google/generative-ai": "^0.24.1",
    "@growthbook/growthbook": "^1.6.2",
    "@vinejs/vine": "^3.0.1",
    "adonis-autoswagger": "^3.73.0",
    "luxon": "^3.6.1",
    "mysql2": "^3.14.1",
    "openai": "^4.104.0",
    "pg": "^8.13.1",
    "pino-roll": "^3.1.0",
    "qiniu": "^7.14.0",
    "reflect-metadata": "^0.2.2",
    "ws": "^8.18.0"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "prettier": "@adonisjs/prettier-config",
  "volta": {
    "node": "24.12.0"
  }
}
